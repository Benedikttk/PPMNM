set terminal svg background "white"
set key bottom right
set output "Plots.svg"
set xlabel "x-values"
set ylabel "y-values"
set tics out
set xzeroaxis
set yzeroaxis
set samples 800
set key right top
set title "Akima (sub-)spline interpolation"


# First plot with points data
plot "testpoints.data" using 1:2 with points pt 6 ps 0.5 title "Not so random data",\
 "Akima.data" using 1:2 with lines linetype 1 lc rgb "red" title "Akima (sub-)spline interpolation fit"


